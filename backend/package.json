{
  "name": "kyc-aml-backend",
  "version": "1.0.0",
  "description": "Clean KYC/AML Backend (Node+TS+Express+Mongo+Mongoose)",
  "main": "dist/server.js",
  "type": "module",
  "scripts": {
    "dev": "nodemon --watch src --ext ts --exec ts-node --files src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "seed": "ts-node src/seed/seed.ts"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "csv-stringify": "^6.5.2",
